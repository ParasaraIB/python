# Definition of savings and yields
savings = 100000
yields = 100000 * (1+0.1) ** 7

# Data type integration
print("I started with $" + str(savings) + " and now have $" + str(yields) + ". Awesome!")

# Definition of pi_string
pi_string = "3.1415926"

# Convert pi_string into float: pi_float
pi_float = float(pi_string)

# Print pi
print(pi_string)
print(pi_float)
